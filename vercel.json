{
  "version": 2,
  "builds": [
    {
      "src": "api/convert-to-cmyk-pdf.js",
      "use": "@vercel/node"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/convert-to-cmyk-pdf",
      "dest": "/api/convert-to-cmyk-pdf.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
